<%- include("partials/header") -%>

    <h1 class="page-heading">Compose</h1>

        <form method="post">
            <div class="input-1 mb-3">
                <label for="compos_title" class="forn-label">Title</label>
                <input type="text" class="compose-input form-control" name="compose_title" placeholder="Write here">
            </div>

            <div class="input-2 mb-3">
                <label for="compose_post" class="form-lable">Post</label>
                <textarea name="compose_post" class="compose-textarea form-control" cols="30" rows="8" placeholder="Write here"></textarea>
            </div>
       
            <button type="submit" class="form-btn btn btn primary">Publish</button>
        </form>
 


    <%- include("partials/footer") -%>