<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

 <!-- Here "element" is equal to post[i] -->

<% posts.forEach((element) => { %>
    <h1><%= element.title %></h1> 
    <p><%= element.body.substring(0, 100) + "..." %><a class="readMoreBtn" href= "<%= /posts/+element.title %>">Read more.</a></p>
<% }) %> 


<%- include("partials/footer") -%>